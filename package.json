{
  "name": "path-design-system",
  "private": true,
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "lerna run clean",
    "build": "lerna run build",
    "build:icons": "lerna run build:icons",
    "reset": "lerna run reset && rm -rf node_modules",
    "setup": "npm i && npm run build && lerna exec npm link",
    "lerna": "lerna",
    "now-build": "npm run build && build-storybook --docs -o public && build-storybook -o public/workshop",
    "deploy:canary": "lerna publish -y --canary --preid canary --dist-tag=canary",
    "storybook": "start-storybook -p 6020",
    "build-storybook": "build-storybook --docs -o public",
    "lerna:lint:fix": "lerna run lint:js:fix",
    "lerna:lint": "lerna run lint:js",
    "lint:fix": "eslint --fix",
    "husky:pre-commit": "lint-staged",
    "husky:commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
    "cypress:open": "npm run build && concurrently -k -n \"DESIGN SYSTEM,CYPRESS\" -c \"blue,magenta\" \"npm run storybook\" \"wait-on tcp:6020 && cypress open\"",
    "cypress:run": "npm run build && concurrently -k -n \"DESIGN SYSTEM,CYPRESS\" -c \"blue,magenta\" --success first \"npm run storybook\" \"wait-on tcp:6020 && cypress run\""
  },
  "repository": {
    "type": "git",
